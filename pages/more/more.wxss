/* pages/more/more.wxss */
page {
  background-color: #f5f5f5;
}

.container {
  height: 100%;
  min-height: 100%;
  overflow: hidden;
  overflow-y: hidden;
  margin-bottom: 25px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}
.user-cart-list {
  background-color: #fff;
  width: 100%;
  text-align: center;
  margin-bottom: 10px;
}

.cart-list {
  display: inline-block;
  width: 25%;
  color: #666;
  font-size: 26rpx;
  margin-top: 10px;
  margin-bottom: 10px;
  position: relative;
}
.cart-list image {
  width: 50rpx;
  height: 50rpx;
  display: block;
  margin: 0 auto;
  padding-bottom: 5px;
}