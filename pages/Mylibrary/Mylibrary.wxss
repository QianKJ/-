/* pages/Mylibrary/Mylibrary.wxss */.container {
  height: 100%;
  min-height: 100%;
  overflow: hidden;
  overflow-y: hidden;
  margin-bottom: 25px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}
.user-cart-list {
  background-color: #fff;
  width: 100%;
  text-align: center;
  margin-bottom:60px;
}

.cart-list {
  display: inline-block;
  width: 40%;
  color: #666;
  font-size: 60rpx;
  margin-top: 10px;
  margin-bottom: 10px;
  position: relative;
}
.cart-list image {
  width: 60px;
  height: 60px;
  display: block;
  margin: 0 auto;
  padding-bottom: 5px;
}
 .UCenter-bg { 
  background-image:linear-gradient(45deg, #ff9700, #ed1c24);
  background-size: cover;
  height: 350rpx;
  display: flex;
 
  overflow: hidden;
  position: relative;
  flex-direction: column;
 
  color: #fff;
  font-weight: 300;
  text-shadow: 0 0 3px rgba(0, 0, 0, 0.3);
} 

